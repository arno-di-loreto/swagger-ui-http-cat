<td width='15%' class='code'><img width="300" src="http://http.cat/{{code}}" alt="{{code}}"/></td>
<td class="markdown">{{{message}}}</td>
<td width='50%'><span class="model-signature" /></td>
<td class="headers">
  <table>
    <tbody>
      {{#each headers}}
      <tr>
        <td>{{@key}}</td>
        <td>{{this.description}}</td>
        <td>{{this.type}}</td>
      </tr>
      {{/each}}
    </tbody>
  </table>
</td>
